{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "lib": [
      "esnext"
    ],
    "strict": true,
    "rootDir": "src",
    "outDir": "dist",
    "plugins": [],
    "typeRoots": [
      "node_modules/@types",
      "types"
    ],
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "strictPropertyInitialization": false
  }
}
